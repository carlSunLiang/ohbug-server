{
  "name": "@ohbug-server/manager",
  "version": "0.0.0",
  "description": "ohbug server manager",
  "author": "chenyueban",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "cross-env NODE_ENV=development nest start --watch",
    "build": "nest build",
    "start:debug": "nest start --debug --watch",
    "start:test": "cross-env pm2 start ./ecosystem.config.js --env test",
    "start:prod": "cross-env pm2 start ./ecosystem.config.js --env production",
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.7.1",
    "@nestjs/bull": "^0.1.1",
    "@nestjs/elasticsearch": "^7.1.0",
    "@nestjs/schedule": "^0.4.0",
    "@ohbug-server/common": "^0.0.0",
    "bull": "^3.14.0"
  },
  "devDependencies": {
    "@types/bull": "^3.13.0"
  }
}
